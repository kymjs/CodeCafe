<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="base_SwipeBackLayout">
        <attr name="base_edge_size" format="dimension"/>
        <attr name="base_edge_flag">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="bottom" value="2"/>
            <enum name="all" value="3"/>
        </attr>
        <attr name="base_shadow_left" format="reference"/>
        <attr name="base_shadow_right" format="reference"/>
        <attr name="base_shadow_bottom" format="reference"/>
    </declare-styleable>

    <attr name="base_SwipeBackLayoutStyle" format="reference"/>
</resources>